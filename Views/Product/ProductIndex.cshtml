@model IEnumerable<Product>

@{
    ViewBag.Title = "ProductIndex";
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script> <!-- JQuery library -->
    <script src="https://cdn.jsdelivr.net/npm/jquery-validation@1.19.3/dist/jquery.validate.min.js"></script>
    <!-- JQuery Validation plugin -->
    <title>Document</title>
</head>

<body>

    <h4>Partial Product Integration</h4>
    <button onclick="loadpartialview()">Click Me</button>
    <div id="partialviewcontainer">
        <partial>@Html.Partial("_ProductDetails",Model)</partial>
    </div>
</body>
<script>
    function loadpartialview() {
        $.ajax({
            url: '@Url.Action("ProductIndex", "Product")',
            type: 'GET',
            success: function (result) {
                $("#partialviewcontainer").html(result); // Corrected ID selector
            },
            error: function (error) {
                alert("Error in Fetching");
            }
        });
    }
</script>

</html>
